# S3
aws s3 ls

#make bucket
aws s3 mb s3://furkanalp-demo

#copy
aws s3 cp in-class.yaml s3://furkanalp-demo

#list buckets
aws s3 ls s3://furkanalp-demo

#remove object
aws s3 rm s3://furkanalp-demo/in-class.yaml

#remove empty bucket
aws s3 rb s3://furkanalp-demo

#remove full bucket
aws s3 rb s3://furkanalp-demo --force  